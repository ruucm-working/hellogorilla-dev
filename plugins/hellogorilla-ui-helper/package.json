    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^1.1.11",
