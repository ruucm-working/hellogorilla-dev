{
  "name": "harbor-ui-plugin",
  "version": "0.1.0",
  "description": "Harbor UI Plugin",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --progress --colors --watch --mode development",
    "build": "NODE_ENV=production webpack --progress --colors --mode production",
    "build-css": "node-sass-chokidar assets/ -o assets/",
    "watch-css": "yarn build-css && node-sass-chokidar assets/ -o assets/ --watch --recursive",
    "build-main-css": "node-sass-chokidar --output-style compressed styles/main.scss styles/main.css",
    "watch-main-css": "yarn build-main-css && node-sass-chokidar --output-style compressed styles/main.scss styles/main.css --watch --recursive",
    "run-all": "./node_modules/.bin/npm-run-all --parallel start watch-main-css"
  },
  "keywords": [
    "wordpress",
    "react",
    "plugin",
    "webpack"
  ],
  "author": "ruucm",
  "license": "GPL-3.0",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "charming": "^2.2.2",
    "gsap": "^2.0.2",
    "imagesloaded": "^4.1.4",
    "intersection-observer-polyfill": "^0.1.0",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.0",
    "react": "15.5.4",
    "react-burger-menu": "^2.5.3",
    "react-dom": "15.5.4",
    "react-dropzone": "^6.2.4",
    "react-ga": "^2.5.0",
    "react-mailchimp-subscribe": "^2.0.1",
    "react-on-screen": "^2.0.4",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-scroll": "^1.7.10",
    "react-select": "^2.1.0",
    "react-slick": "^0.23.1",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "ruucm-blocks": "https://github.com/ruucm/ruucm-blocks",
    "ruucm-util": "^0.12.0",
    "styled-components": "3.3.0",
    "superagent": "^3.8.3",
    "woocommerce-api": "^1.4.2",
    "wpapi": "^1.1.2"
